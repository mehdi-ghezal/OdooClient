{
    "name": "jacobsteringa/odoo-client",
    "type": "library",
    "description": "A PHP Client for Odoo",
    "keywords": ["odoo", "openerp"],
    "homepage": "https://github.com/jacobsteringa/OdooClient",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Steringa",
            "email": "jacobsteringa@gmail.com"
        },
        {
            "name": "Mehdi GHEZAL",
