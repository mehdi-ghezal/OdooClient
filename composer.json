        }
    ],
    "require": {
        "php": ">=5.5.9",

        "zendframework/zend-xmlrpc": "~2.3",
        "zendframework/zendxml": "1.0.*",
        "zendframework/zend-http": "~2.3",
        "symfony/options-resolver": "^3.2",
        "psr/simple-cache": "~1.0",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-4": { "Jsg\\Odoo\\": "src/" }
